<ul >
    <% allTheBeers.forEach(beer=> { %>
    <li class="allBeersList">
        <div class="divImage">
            <img class="images" src="<%- beer.image_url %>">
        </div>
        <div>
            <h2><%- beer.name %></h2>
            <p><%- beer.description %></p>
            <p><%- beer.tagline %></p>
            <p><%- beer.food_pairing %></p>
        </div>
    </li>

   <% }) %>

</ul>